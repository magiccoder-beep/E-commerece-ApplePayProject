{
  "compileOnSave": true,
  "compilerOptions": {
    "inlineSourceMap": true,
    "lib": [ "dom", "es6" ],
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "removeComments": true,
    "target": "es5"
  },
  "exclude": [
    "bin",
    "node_modules",
    "obj"
  ]
}
